# Copy this file to .env and fill in your credentials
OPENAI_API_KEY=your-openai-key
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-key

# Goal-Driven System (required for Enhanced Director)
ENABLE_GOAL_DRIVEN_SYSTEM=true
AUTO_CREATE_GOALS_FROM_WORKSPACE=true
GOAL_VALIDATION_INTERVAL_MINUTES=20

# Quality Assurance
ENABLE_AI_QUALITY_ASSURANCE=true
ENABLE_DYNAMIC_AI_ANALYSIS=true

# Enhanced Director Configuration
ENABLE_ENHANCED_DIRECTOR=true
DIRECTOR_USE_STRATEGIC_GOALS=true

# Enhanced Reasoning Configuration (o3/Claude style)
ENABLE_DEEP_REASONING=true
DEEP_REASONING_THRESHOLD=0.7        # Complexity threshold to trigger deep reasoning
REASONING_CONFIDENCE_MIN=0.6        # Minimum confidence level for recommendations
MAX_REASONING_ALTERNATIVES=3        # Max alternatives to evaluate

# Rate Limiting & Performance
OPENAI_RPM_LIMIT=3000              # Requests per minute limit
VALIDATION_CACHE_TTL=600           # Cache validation results for 10 minutes
ENABLE_AGGRESSIVE_CACHING=true     # Enable intelligent caching
AUTO_REFRESH_INTERVAL=600          # Frontend auto-refresh every 10 minutes
