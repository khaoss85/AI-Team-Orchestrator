{
  "name": "ai-team-orchestrator-book",
  "version": "1.0.0",
  "description": "AI Team Orchestrator - Premium Book Web & PDF Edition",
  "main": "convert_to_pdf.js",
  "scripts": {
    "pdf": "node convert_to_pdf.js",
    "pdf:en": "node generate-pdf.js en",
    "pdf:it": "node generate-pdf.js it",
    "pdf:both": "npm run pdf:en && npm run pdf:it",
    "serve": "python3 -m http.server 8888",
    "build-pdf": "npm install && npm run pdf:both",
    "dev": "python3 -m http.server 8888"
  },
  "dependencies": {
    "puppeteer": "^21.6.1",
    "vercel": "^46.1.1"
  },
  "keywords": [
    "ai",
    "orchestrator",
    "book",
    "pdf",
    "web",
    "premium"
  ],
  "author": "Daniele Pelleri",
  "license": "Proprietary",
  "repository": {
    "type": "git",
    "url": "private"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
